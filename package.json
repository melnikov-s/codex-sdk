  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/lib.js",
  "module": "./dist/lib.js",
  "exports": {
    ".": {
      "types": "./dist/src/lib.d.ts",
      "default": "./dist/lib.js"
    }
  },
  "types": "./dist/src/lib.d.ts",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "format": "pnpm exec prettier --check src tests",
    "format:fix": "pnpm exec prettier --write src tests",
    "dev": "pnpm exec tsc --watch",
    "lint": "pnpm exec eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives",
    "lint:fix": "pnpm exec eslint src tests --ext ts --ext tsx --fix",
    "test": "pnpm exec vitest run",
    "test:watch": "pnpm exec vitest --watch",
    "typecheck": "pnpm exec tsc --noEmit",
    "build": "node build.mjs"
  },
  "files": [
    "dist"
  ],
